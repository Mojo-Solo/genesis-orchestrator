            "refId": "B"
          },
          {
            "expr": "2000",
            "legendFormat": "Response Time Target (ms)",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, avg_over_time(rate(genesis_orchestrator_request_duration_seconds_bucket[$timeframe])[5m:1m])) * 1000",
            "legendFormat": "Actual P95 Response Time (ms)",
            "refId": "D"
          },
          {
            "expr": "1.0",
            "legendFormat": "Error Rate Target (%)",
            "refId": "E"
          },
          {
            "expr": "avg_over_time(sum(rate(genesis_orchestrator_failed_runs[$timeframe])) / sum(rate(genesis_orchestrator_total_runs[$timeframe]))[5m:1m]) * 100",
            "legendFormat": "Actual Error Rate (%)",
            "refId": "F"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "continuous-GrYlRd"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
