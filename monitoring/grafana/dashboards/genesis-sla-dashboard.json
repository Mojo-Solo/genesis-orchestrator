{
  "dashboard": {
    "id": null,
    "title": "GENESIS Orchestrator - SLA Dashboard",
    "tags": ["genesis", "sla", "reliability"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "timeframe",
          "type": "custom",
          "options": [
            {
              "text": "Last Hour",
              "value": "1h"
            },
            {
              "text": "Last 6 Hours",
              "value": "6h"
            },
            {
              "text": "Last 24 Hours",
              "value": "24h"
            },
            {
              "text": "Last 7 Days",
              "value": "7d"
            },
            {
              "text": "Last 30 Days",
              "value": "30d"
            }
          ],
          "current": {
            "text": "Last 24 Hours",
            "value": "24h"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "SLA Compliance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "(avg_over_time(up{job=\"genesis-orchestrator\"}[$timeframe]) * 100)",
            "legendFormat": "Availability %",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, avg_over_time(rate(genesis_orchestrator_request_duration_seconds_bucket[$timeframe])[5m:1m])) * 1000",
            "legendFormat": "P95 Response Time (ms)",
            "refId": "B"
          },
          {
            "expr": "(avg_over_time(sum(rate(genesis_orchestrator_failed_runs[$timeframe])) / sum(rate(genesis_orchestrator_total_runs[$timeframe]))[5m:1m]) * 100)",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 98
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent",
            "decimals": 3
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "P95 Response Time (ms)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "ms"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 1000
                      },
                      {
                        "color": "red",
                        "value": 2000
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Error Rate %"
              },
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 0.5
                      },
                      {
                        "color": "red",
                        "value": 1.0
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "SLA Target vs Actual",
        "type": "bargauge",
        "targets": [
          {
            "expr": "99.9",
            "legendFormat": "Availability Target",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(up{job=\"genesis-orchestrator\"}[$timeframe]) * 100",
