{
  "dashboard": {
    "id": null,
    "title": "Genesis Orchestrator - Overview",
    "tags": ["genesis", "orchestrator", "overview"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "System Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"genesis-orchestrator\"}",
            "legendFormat": "Orchestrator Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "none"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(genesis_orchestrator_successful_runs / genesis_orchestrator_total_runs) * 100",
            "legendFormat": "Success Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 4,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 3,
        "title": "Average Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "genesis_orchestrator_average_latency_ms",
            "legendFormat": "Avg Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3000
                },
                {
                  "color": "red",
                  "value": 5000
                }
              ]
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 8,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 4,
        "title": "Stability Score",
        "type": "stat",
        "targets": [
          {
            "expr": "genesis_stability_current_score * 100",
            "legendFormat": "Stability Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 12,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 5,
        "title": "Token Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "genesis_orchestrator_total_tokens_used",
            "legendFormat": "Total Tokens",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1000000
                },
                {
                  "color": "red",
                  "value": 5000000
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 16,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 6,
        "title": "SLA Uptime (99.9% Target)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg_over_time(up{job=\"genesis-orchestrator\"}[24h]) * 100",
            "legendFormat": "24h Uptime",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 99.5
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent",
            "decimals": 3,
            "min": 99,
            "max": 100
          }
        },
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 20,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 7,
        "title": "Request Rate & Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(genesis_orchestrator_total_runs[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          },
          {
            "expr": "genesis_orchestrator_average_latency_ms",
            "legendFormat": "Avg Latency (ms)",
            "refId": "B",
            "yAxis": 2
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          },
          {
            "label": "Latency (ms)",
            "min": 0,
            "opposite": true
          }
        ],
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
        }
      },
      {
        "id": 8,
        "title": "Success Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "(rate(genesis_orchestrator_successful_runs[5m]) / rate(genesis_orchestrator_total_runs[5m])) * 100",
            "legendFormat": "Success Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "yAxes": [
          {
            "label": "Success Rate %",
            "min": 0,
            "max": 100
          }
        ],
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
        }
      },
      {
        "id": 9,
        "title": "Router Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "genesis_router_avg_selection_time_ms",
            "legendFormat": "Selection Time (ms)",
            "refId": "A"
          },
          {
            "expr": "genesis_router_efficiency_gain",
            "legendFormat": "Efficiency Gain %",
            "refId": "B",
            "yAxis": 2
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "yAxes": [
          {
            "label": "Selection Time (ms)",
            "min": 0
          },
          {
            "label": "Efficiency %",
            "min": 0,
            "opposite": true
          }
        ]
      },
      {
        "id": 10,
        "title": "Security Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(genesis_security_violations_24h[1h])",
            "legendFormat": "Security Violations/hour",
            "refId": "A"
          },
          {
            "expr": "increase(genesis_security_auth_failures_24h[1h])",
            "legendFormat": "Auth Failures/hour",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ]
      }
    ]
  }
}