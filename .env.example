# GENESIS Orchestrator Environment Variables
# Copy this file to .env and fill in your values

# API Keys (Store these as GitHub Secrets)
CLAUDE_API_KEY=your-claude-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=genesis_orchestrator
DB_USERNAME=root
DB_PASSWORD=

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Temporal Configuration
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=genesis-orchestrator-queue

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# Security
HMAC_SECRET=your-hmac-secret-here
ENCRYPTION_KEY=your-encryption-key-here
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60

# Deployment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=info

# GitHub Actions
GITHUB_TOKEN=your-github-token-here
GITHUB_REPOSITORY=Mojo-Solo/genesis-orchestrator

# Seeds for Reproducibility
GENESIS_SEED=42
GENESIS_TEMPERATURE=0.1
GENESIS_TOP_P=0.95