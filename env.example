# Temporal Configuration
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=genesis-orchestrator-queue

# Database Configuration  
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=genesis_orchestrator
DB_USERNAME=root
DB_PASSWORD=

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Monitoring
ENABLE_MONITORING=true
ENABLE_META_LEARNING=true
LOG_LEVEL=INFO

# OpenTelemetry
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
OTEL_SERVICE_NAME=genesis-orchestrator
OTEL_METRICS_EXPORTER=prometheus
OTEL_TRACES_EXPORTER=jaeger

# Security
AUDIT_LOG_DRIVER=database
ENABLE_PII_REDACTION=true
ENABLE_HMAC_VALIDATION=true
HMAC_SECRET_KEY=

# API Keys (use GitHub Secrets in production)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
PINECONE_API_KEY=